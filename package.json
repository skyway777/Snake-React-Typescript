{
  "name": "snake-game",
  "version": "0.1.0",
  "description": "Snake game on Typescript example",
  "author": "skyway",
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run clean && webpack-dashboard -- webpack --config configs/webpack.config.js --env.build --env.sourceMap",
    "dev": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open",
    "start": "yarn dev",
    "lint": "tslint --project tsconfig.json",
    "tsc": "tsc -p . --noEmit",
    "tsc:watch": "tsc -p . --noEmit -w",
    "test": "jest --config jest.json",
    "test:update": "jest --config jest.json -u",
    "reinstall": "rm -rf node_modules && yarn",
    "reinstall:win": "rd /s /q node_modules && yarn",
    "precommit": "yarn run lint",
    "prepush": "yarn run lint & yarn run tsc & yarn run test",
    "deploy": "echo 'deploy not set!'"
  },
  "dependencies": {
    "@types/prop-types": "",
    "css-loader": "^0.28.7",
    "csstips": "^0.2.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "moment": "^2.19.2",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.0",
    "raw-loader": "^0.5.1",
    "react": "16",
    "react-dom": "^16.0.0",
    "react-fontawesome": "^1.6.1",
    "react-input-range": "^1.2.2",
    "react-rangeslider": "^2.2.0",
    "react-router-dom": "^4.2.2",
    "reactrangeslider": "^3.0.6",
    "redux": "^3.6.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "tslib": "^1.6.1",
    "typestyle": "^1.3.2"
  },
  "devDependencies": {
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/react-hot-loader": "^3.0.1",
    "@types/webpack": "^3.8.1",
    "@types/webpack-dev-server": "^2.9.2",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.1.3",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "ts-jest": "^21.2.3",
    "ts-loader": "^3.1.1",
    "tslint": "^5.1.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.2",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.3.0"
  }
}
